# Cloudflare Pages Redirects

# Block admin access attempts
/admin/*  /  404
/wp-admin/*  /  404
/wp-login.php  /  404
/.env  /  404
/config.php  /  404

# Prevent trailing slash redirects (keep URLs as-is)
/application.html  /application/  301
/vpn.html  /vpn/  301

# Keep old paths working
/application  /application/  200
/vpn  /vpn/  200
